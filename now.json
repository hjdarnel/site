{
  "version": 2,
  "name": "namesite",
  "alias": ["https://darnell.io"],
  "builds": [
    {
      "src": "blog/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    },
    {
      "src": "www/index.html",
      "use": "@now/static"
    }
  ],
  "routes": [
      { "src": "/blog/(.*)", "dest": "/blog/$1" },
      { "src": "/(.*)", "dest": "/www/$1" }
  ]
}